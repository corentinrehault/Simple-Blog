<?xml version="1.0" encoding="ISO-8859-1" ?> 
<!DOCTYPE tiles-definitions PUBLIC  
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">


<!-- templateblog frontoffice -->
<tiles-definitions>

	<definition name="rootTemplate" template="root.jsp">

		<put-attribute name="head" value="/frontOffice/templates/head.jsp" />

		<put-attribute name="zone_titre"
			value="/frontOffice/templates/zone_titre.jsp" />

		<put-attribute name="zone_login"
			value="/frontOffice/templates/zone_login.jsp" />

		<put-attribute name="zone_posts"
			value="/frontOffice/contents/zone_posts.jsp" />


		<put-attribute name="zone_pages"
			value="/frontOffice/templates/zone_pages.jsp" />

		<put-attribute name="zone_nav"
			value="/frontOffice/templates/zone_nav.jsp" />

		<put-attribute name="zone_pied"
			value="/frontOffice/templates/zone_pied.jsp" />

		<put-attribute name="footStrap"
			value="/frontOffice/templates/footStrap.jsp" />

	</definition>



	<definition name="adminTemplate" template="/backOffice/admin.jsp">



		<put-attribute name="bloglogin"
			value="/backOffice/templates/adminTestSecure.jsp" />


	</definition>



</tiles-definitions>