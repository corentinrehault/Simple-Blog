	<!-- memoryStoreEvictionPolicy="LRU" /> -->

	<defaultCache maxEntriesLocalHeap="0" eternal="false"
		timeToIdleSeconds="1200" timeToLiveSeconds="1200" />

	<cache name="styleCache" maxEntriesLocalHeap="10000"
		maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off" />

	<cache name="interetCache" maxEntriesLocalHeap="10000"
		maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off" />

	<cache name="paysCache" maxEntriesLocalHeap="10000"
		maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off" />

</ehcache>